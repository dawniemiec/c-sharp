<Window x:Class="lab6dom.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab6dom"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="linq1" Content="L1" HorizontalAlignment="Left" Margin="35,50,0,0" VerticalAlignment="Top" Click="linq1_Click"/>
        <Button x:Name="linq2" Content="L2" HorizontalAlignment="Left" Margin="55,50,0,0" VerticalAlignment="Top" Click="linq2_Click"/>
        <Button x:Name="linq3" Content="L3" HorizontalAlignment="Left" Margin="75,50,0,0" VerticalAlignment="Top" Click="linq3_Click"/>
        <Button x:Name="linq4" Content="L4" HorizontalAlignment="Left" Margin="95,50,0,0" VerticalAlignment="Top" Click="linq4_Click"/>
        <Button x:Name="linq5" Content="L5" HorizontalAlignment="Left" Margin="115,50,0,0" VerticalAlignment="Top" Click="linq5_Click"/>
        <Button x:Name="minmax" Content="min/max" HorizontalAlignment="Left" Margin="145,50,0,0" VerticalAlignment="Top" Click="minmax_Click"/>
        <Button x:Name="action" Content="action" HorizontalAlignment="Left" Margin="205,50,0,0" VerticalAlignment="Top" Click="action_Click"/>
        <ListBox x:Name="lBox" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="35,140,434,106"/>
        <Label x:Name="actionL" Content="actionLabel" HorizontalAlignment="Left" Margin="263,47,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
